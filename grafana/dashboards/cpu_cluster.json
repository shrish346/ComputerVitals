{
  "uid": "cpu-cluster",
  "title": "CPU Cluster Health",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "style": "dark",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "CPU Utilization (%) by Node",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Load Average (1m, 5m, 15m) by Node",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "node_load1"
        },
        {
          "refId": "B",
          "expr": "node_load5"
        },
        {
          "refId": "C",
          "expr": "node_load15"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    },
    {
      "type": "timeseries",
      "title": "CPU IO Wait (%) by Node",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg by (instance) (rate(node_cpu_seconds_total{mode=\"iowait\"}[5m])) * 100"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      }
    }
  ],
  "templating": {
    "list": []
  }
}

